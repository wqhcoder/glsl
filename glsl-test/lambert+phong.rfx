     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec4 position;
varying vec4 color;

struct Fog
{
    float startDistance;
    float endDistance;
    float density;
    vec4 color;
};

void main(void)
{
   Fog fog;
   fog.startDistance = 10.0f;
   fog.endDistance = 500.0f;
   fog.density = 0.5f;
   fog.color = vec4(1.0f, 0.0f, 0.0f, 0.3f);
   
   float dist = abs(position.z);
   float fogFactor = (fog.endDistance - dist) / (fog.endDistance - fog.startDistance);
   fogFactor = clamp(fogFactor, 0.0f, 1.0f);
   vec4 fragColor = mix(fog.color, color, fogFactor);
   gl_FragColor = fragColor;
   
}]]></RmShaderCode></RmGLShader>
